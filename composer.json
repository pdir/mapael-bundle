{
  "name": "pdir/mapael-bundle",
  "type":"contao-bundle",
  "description": "mapael bundle for Contao 4",
  "authors": [
    {
      "name": "Webdesign f√ºr Dresden",
      "homepage": "https://pdir.de",
      "email" : "develop@pdir.de",
      "role" : "Developer"
    }
  ],
  "require": {
    "php": "^5.6 || ^7.0",
    "contao/core-bundle": "^4.4"
  },
  "conflict": {
    "contao/core": "*"
  },
  "autoload": {
    "psr-4": {
      "Pdir\\MapaelBundle\\": "src/"
    },
    "classmap": [
      "src/Resources/contao/"
    ],
    "exclude-from-classmap": [
      "src/Resources/contao/config/",
      "src/Resources/contao/dca/",
      "src/Resources/contao/languages/",
      "src/Resources/contao/templates/"
    ]
  },
  "extra": {
    "contao-manager-plugin": "Pdir\\MapaelBundle\\ContaoManager\\Plugin"
  }
}